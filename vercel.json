{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
        "buildCommand": "echo 'Build already completed in installCommand'"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "YARN_ENABLE": "false"
  }
}